import com.rosy.punish.dao.VerifyCheckupDao;

public interface VerifyCheckupManager extends Manager {
    /**
     * Retrieves all of the verifyCheckups
     */
    public List getVerifyCheckups(VerifyCheckup verifyCheckup);

    /**
