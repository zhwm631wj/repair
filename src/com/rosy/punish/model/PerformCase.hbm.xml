<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.rosy.punish.model.PerformCase"
            table="performcase"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.String"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid">

    </generator>

    </id>

        <property
            name="caseFine"
                    column="caseFine"
                length="11"
        >

        </property>

        <property
            name="confiscateProperty"
                    column="confiscateProperty"
                length="2000"
        >

        </property>

        <property
            name="infine"
                    column="inFine"
                length="11"
        >

        </property>

        <property
            name="notInFine"
                    column="notInFine"
                length="11"
        >

        </property>

        <property
            name="confiscateState"
                    column="confiscateState"
                length="1"
        >

        </property>

        <property
            name="propertyKeep"
                    column="propertyKeep"
                length="400"
        >

        </property>

        <property
            name="decideMeasure"
                    column="decideMeasure"
                length="1"
        >

        </property>

        <property
            name="remark"
                    column="remark"
                length="200"
        >

        </property>

        <property
            name="rflag"
                    column="rFlag"
                length="1"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                length="19"
        >

        </property>

    </class>    

</hibernate-mapping>

