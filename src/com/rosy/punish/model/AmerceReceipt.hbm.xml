<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.rosy.punish.model.AmerceReceipt"
            table="amercereceipt"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.String"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag insteaof--><generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="serialNum"
                    column="serialNum"
                length="30"
        >

        </property>

        <property
            name="adminOrg"
                    column="adminOrg"
                length="50"
        >

        </property>

        <property
            name="payOrg"
                    column="payOrg"
                length="50"
        >

        </property>

        <property
            name="fine"
                    column="fine"
                length="11"
        >

        </property>

        <property
            name="confiscateSum"
                    column="confiscateSum"
                length="11"
        >

        </property>

        <property
            name="total"
                    column="total"
                length="15"
        >

        </property>

        <property
            name="inceptBank"
                    column="inceptBank"
                length="150"
        >

        </property>

        <property
            name="inceptMan"
                    column="inceptMan"
                length="30"
        >

        </property>

        <property
            name="checkMan"
                    column="checkMan"
                length="30"
        >

        </property>

        <property
            name="inceptDate"
                    column="inceptDate"
                length="19"
        >

        </property>

        <property
            name="remark"
                    column="remark"
                length="200"
        >

        </property>

        <property
            name="rflag"
                    column="rFlag"
                length="1"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                length="19"
        >

        </property>

    </class>    

</hibernate-mapping>

