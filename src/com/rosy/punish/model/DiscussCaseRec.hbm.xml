<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.rosy.punish.model.DiscussCaseRec"
            table="discusscaserec"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.String"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag instead -->
    <generator class="uuid">

    </generator>

    </id>

        <property
            name="discussContent"
                    column="discussContent"
                length="8000"
        >

        </property>

        <property
            name="discussResult"
                    column="discussResult"
                length="1000"
        >

        </property>

        <property
            name="joinPersonnel"
                    column="joinPersonnel"
                length="1000"
        >

        </property>

        <property
            name="meetingDate"
                    column="meetingDate"
                length="19"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                length="19"
        >

        </property>

        <property
            name="rflag"
                    column="rFlag"
                length="1"
        >

        </property>

        <property
            name="remark"
                    column="remark"
                length="200"
        >

        </property>

    </class>    

</hibernate-mapping>

