<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.rosy.punish.model.PunishDraft"
            table="punishdraft"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.String"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag insteaof--><generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="depNum"
                    column="depNum"
                length="12"
        >

        </property>

        <property
            name="adminOrder"
                    column="adminOrder"
                length="2"
        >

        </property>

        <property
            name="keyWord"
                    column="keyWord"
                length="4"
        >

        </property>

        <property
            name="serialNum"
                    column="serialNum"
                length="4"
        >

        </property>

        <property
            name="writContent"
                    column="writContent"
                length="8000"
        >

        </property>

        <property
            name="draftDate"
                    column="draftDate"
                length="19"
        >

        </property>

        <property
            name="lawPartIdea"
                    column="lawPartIdea"
                length="200"
        >

        </property>

        <property
            name="lawPartMan"
                    column="lawPartMan"
                length="30"
        >

        </property>

        <property
            name="lawPartCheckDate"
                    column="lawPartCheckDate"
                length="19"
        >

        </property>

        <property
            name="lawPartOrg"
                    column="lawPartOrg"
                length="50"
        >

        </property>

        <property
            name="directorGeneralIdea"
                    column="directorGeneralIdea"
                length="200"
        >

        </property>

        <property
            name="directorGeneral"
                    column="directorGeneral"
                length="30"
        >

        </property>

        <property
            name="directorCheckDate"
                    column="directorCheckDate"
                length="19"
        >

        </property>

        <property
            name="directorOrg"
                    column="directorOrg"
                length="50"
        >

        </property>

        <property
            name="remark"
                    column="remark"
                length="200"
        >

        </property>

        <property
            name="rflag"
                    column="rFlag"
                length="1"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                length="19"
        >

        </property>

    </class>    

</hibernate-mapping>

