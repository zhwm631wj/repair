<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.rosy.punish.model.PunishDecisionBook"
            table="punishdecisionbook"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.String"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag insteaof--><generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="depNum"
                    column="depNum"
                length="12"
        >

        </property>

        <property
            name="adminOrder"
                    column="adminOrder"
                length="2"
        >

        </property>

        <property
            name="writContent"
                    column="writContent"
                length="6000"
        >

        </property>

        <property
            name="remark"
                    column="remark"
                length="200"
        >

        </property>

        <property
            name="rflag"
                    column="rFlag"
                length="1"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                length="19"
        >

        </property>

    </class>    

</hibernate-mapping>

