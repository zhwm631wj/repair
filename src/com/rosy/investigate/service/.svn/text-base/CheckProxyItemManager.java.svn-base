
package com.rosy.investigate.service;

import java.util.List;

import com.rosy.service.Manager;
import com.rosy.investigate.model.CheckProxyItem;
import com.rosy.investigate.dao.CheckProxyItemDao;

public interface CheckProxyItemManager extends Manager {
    /**
     * Retrieves all of the checkProxyItems
     */
    public List getCheckProxyItems(CheckProxyItem checkProxyItem);

    /**
     * Gets checkProxyItem's information based on id.
     * @param id the checkProxyItem's id
