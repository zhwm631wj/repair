<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping

>

    <class
            name="com.rosy.register.model.SceneCheck"
            table="scenecheck"
    >

    <id
        name="id"
            column="ID"
                type="java.lang.String"
    >

    <!-- The generator-class attribute of @hibernate.id is deprecated, use the @hibernate.generator tag insteaof--><generator class="uuid.hex">

    </generator>

    </id>

        <property
            name="eventId"
                    column="eventID"
                length="48"
        >

        </property>

        <property
            name="startTime"
                    column="startTime"
                length="19"
        >

        </property>

        <property
            name="endTime"
                    column="endTime"
                length="19"
        >

        </property>

        <property
            name="checkWhere"
                    column="checkWhere"
                length="100"
        >

        </property>

        <property
            name="checkMan"
                    column="checkMan"
                length="100"
        >

        </property>

        <property
            name="party"
                    column="party"
                length="30"
        >

        </property>

        <property
            name="checkPurpose"
                    column="checkPurpose"
                length="100"
        >

        </property>

        <property
            name="checkInstance"
                    column="checkInstance"
                length="4000"
        >

        </property>

        <property
            name="eyewithness"
                    column="eyewithness"
                length="30"
        >

        </property>

        <property
            name="notesKey"
                    column="notesKey"
                length="48"
        >

        </property>

        <property
            name="remark"
                    column="remark"
                length="200"
        >

        </property>

        <property
            name="rflag"
                    column="rFlag"
                length="1"
        >

        </property>

        <property
            name="createDate"
                    column="createDate"
                length="19"
        >

        </property>

        <property
            name="caseStatus"
                    column="caseStatus"
                length="1"
        >

        </property>

    </class>    

</hibernate-mapping>

